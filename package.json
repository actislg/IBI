{
  "name": "ApplicationFrameworkManagement",
  "version": "1.0.4",
  "description": "Application Framework Management",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "build",
    "release": "build"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "publish": {
    "provider": "github",
    "repo": "ibi-geny",
    "private": false
  },
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-builder": "^19.24.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rpannell/ibi-geny.git"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "bootstrap-table": "^1.11.2",
    "commander": "^2.9.0",
    "electron-config": "^0.2.1",
    "electron-ipc": "^0.3.0",
    "electron-json-storage": "^3.0.7",
    "electron-log": "^2.2.9",
    "electron-updater": "^2.8.9",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "jquery-migrate": "^3.0.0",
    "jquery-ui": "^1.12.1",
    "jquery-validation": "^1.16.0",
    "jsonfile": "^3.0.1",
    "mssql": "^4.0.1",
    "npmi": "^3.0.0",
    "q": "^1.5.0",
    "yeoman-environment": "^1.6.6"
  },
  "build": {
    "appId": "yourappid",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "publish": [
        "github"
      ],
      "target": "nsis",
      "icon": "build/iconA.ico"
    }
  }
}
